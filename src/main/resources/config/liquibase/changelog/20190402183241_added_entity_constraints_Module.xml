<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Module.
    -->
    <changeSet id="20190402183241-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="module_id"
                                 baseTableName="module"
                                 constraintName="fk_module_module_id"
                                 referencedColumnNames="id"
                                 referencedTableName="module"/>

        <addForeignKeyConstraint baseColumnNames="module_type_id"
                                 baseTableName="module"
                                 constraintName="fk_module_module_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="module_type"/>

        <addForeignKeyConstraint baseColumnNames="tag_id"
                                 baseTableName="module"
                                 constraintName="fk_module_tag_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tag"/>

        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="module"
                                 constraintName="fk_module_company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"/>

    </changeSet>
</databaseChangeLog>
